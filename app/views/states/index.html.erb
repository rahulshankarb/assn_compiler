<h1>Results</h1>

<table class="table table-condensed table-bordered table-striped">
  <tr>
    <th>id</th>
    <th>Acronym</th>
    <th>Result</th>
   </tr>

<% @states.each do |state| %>
  <tr>
      <td><%= state.id %></td>
      <td><%= state.acronym %></td>
      <td><%= state.name %></td>
     </tr>
<% end %>
</table>



